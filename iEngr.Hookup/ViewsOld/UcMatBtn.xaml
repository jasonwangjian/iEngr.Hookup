<UserControl x:Class="iEngr.Hookup.Views.UcMatBtn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iEngr.Hookup.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel Orientation="Horizontal">
        <Button Content="Query" Command="{Binding QueryCommand}" Margin="5" Width="80"/>
        <Button Content="Test" Command="{Binding TestCommand}" Margin="5" Width="80"/>
    </WrapPanel>
</UserControl>
